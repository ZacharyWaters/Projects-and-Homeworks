    <test-case>
        <name>reverse_odd([10,-8,105,4,0,-54,12,76,-1,34], 0, 9)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <fully-randomize>1</fully-randomize>
        <input>
            <test-value>
                <address>ARRAY</address>
                <value>x7000</value>
            </test-value>
            <test-array>
                <address>ARRAY</address>
                <value>10,-8,105,4,0,-54,12,76,-1,34</value>
            </test-array>
            <test-subr>
                <name>reverse_odd</name>
                <stack>xF000</stack>
                <r5>xCAFE</r5>
                <r7>x8000</r7>
                <params>x7000, 0, 9</params>
            </test-subr>
        </input>
        <output>
            <test-array points="5">
                <address>ARRAY</address>
                <value>34,-1,76,12,-54,0,4,105,-8,10</value>
            </test-array>
            <test-subr>
                <answer>2</answer>
                <calls>
                    <call><name>reverse_odd</name><params>x7000, 1, 8</params></call>
                </calls>
                <points>
                    <answer>2</answer>
                    <calls>3</calls>
                    <r7>1</r7>
                    <r6>1</r6>
                    <r5>1</r5>
                    <params>2</params>
                    <read-answer>1</read-answer>
                </points>
                <deductions-per-mistake>1</deductions-per-mistake>
            </test-subr>
        </output>
    </test-case>
