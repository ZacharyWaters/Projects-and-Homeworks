<test-suite>
    <test-case>
        <name>reverse("To Hell With Georgia\n")</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-string>
                <address>STRING</address>
                <value>To Hell With Georgia\n</value>
            </test-string>
            <test-value>
                <address>LENGTH</address>
                <value>21</value>
            </test-value>
        </input>
        <output>
            <test-string condition="equals" points="6">
                <address>STRING</address>
                <value>aigroeG htiW lleH oT\n</value>
            </test-string>
        </output>
    </test-case>
    <test-case>
        <name>reverse("THWG\n")</name>
        <has-max-executions>1</has-max-executions>
