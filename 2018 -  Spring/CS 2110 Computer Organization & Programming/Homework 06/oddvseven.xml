<test-suite>
    <test-case>
        <name>oddvseven([0, 1, 2])</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-array>
                <address>ARRAY</address>
                <value>0, 1, 2</value>
            </test-array>
            <test-value>
                <address>LENGTH</address>
                <value>3</value>
            </test-value>
        </input>
        <output>
            <test-stdout condition="equals" points="4">
                <value>ZERO\nODD\nEVEN\n</value>
            </test-stdout>
        </output>
    </test-case>
    <test-case>
