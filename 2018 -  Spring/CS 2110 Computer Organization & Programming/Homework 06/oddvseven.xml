                <value>-1, -2, 0, 1, 2</value>
            </test-array>
            <test-value>
                <address>LENGTH</address>
                <value>5</value>
            </test-value>
        </input>
        <output>
            <test-stdout condition="equals" points="4">
                <value>ODD\nEVEN\nZERO\nODD\nEVEN\n</value>
            </test-stdout>
        </output>
    </test-case>
